              AnchorPane.topAnchor="59.0">
            <children>
                <Label text="Connected to">
                    <font>
                        <Font name="System Bold" size="13.0"/>
                    </font>
                </Label>
                <Label fx:id="systemNameLabel">
                    <padding>
                        <Insets left="20.0"/>
                    </padding>
                </Label>
                <Label text="SystemTime">
                    <font>
                        <Font name="System Bold" size="13.0"/>
                    </font>
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Label>
                <Label fx:id="systemTimeLabel">
                    <padding>
                        <Insets left="20.0"/>
                    </padding>
                </Label>
                <Label text="GPS - Information">
                    <font>
                        <Font name="System Bold" size="13.0"/>
                    </font>
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Label>
                <Label fx:id="gpsPositionLabel" prefHeight="36.0">
                    <padding>
                        <Insets left="20.0"/>
                    </padding>
                </Label>
                <Label text="Command Executing:">
                    <font>
                        <Font name="System Bold" size="13.0"/>
                    </font>
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Label>
                <Label fx:id="currentCommandLabel">
                    <padding>
                        <Insets left="20.0"/>
                    </padding>
                </Label>
            </children>
        </VBox>
        <HBox layoutX="1049.0" layoutY="280.0" prefWidth="215.0" AnchorPane.rightAnchor="15.0">
            <children>
                <TextField fx:id="sendAnyCommandField" prefHeight="26.0"/>
