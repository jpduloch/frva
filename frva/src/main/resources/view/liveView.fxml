                        <Button fx:id="setTimeButton" mnemonicParsing="false" prefWidth="126.0" text="Set time"
                                VBox.vgrow="ALWAYS"/>
                        <Button fx:id="commandCButton" mnemonicParsing="false" prefWidth="126.0" text="Command-C"
                                VBox.vgrow="ALWAYS"/>
                    </children>
                </VBox>
                <VBox alignment="CENTER" HBox.hgrow="ALWAYS">
                    <children>
                        <Button mnemonicParsing="false" prefWidth="126.0" text="Change IT" VBox.vgrow="ALWAYS"/>
