            <children>
                <LineChart fx:id="datachart" alternativeColumnFillVisible="true" axisSortingPolicy="NONE"
                           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                           AnchorPane.topAnchor="0.0">
                    <xAxis>
                        <NumberAxis tickUnit="100" fx:id="xaxis"/>
                    </xAxis>
                    <yAxis>
                        <NumberAxis fx:id="yaxis" tickUnit="1"/>
                    </yAxis>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </LineChart>
            </children>
        </AnchorPane>
        <TextArea fx:id="miniTerminalTextArea" layoutX="1050.0" layoutY="422.0" prefHeight="200.0" prefWidth="215.0"
                  styleClass="terminalMiniView" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0"
                  AnchorPane.topAnchor="444.0"/>
